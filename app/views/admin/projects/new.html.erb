<div class="row">
  <div class="col-md-offset-1 col-md-9">
    <h1>New Project</h1>
    <%= form_for @project, url: { controller: 'projects', action: :create } do |f| %>
      <%= render partial: 'form', locals: { f: f  } %>
      <%= f.submit "Save Project", class: "btn btn-primary" %>
    <% end %>
  </div>
</div><!-- row -->